<div class="row">
    <div class="col-lg-12">
        <h3 class="page-title">Patient</h3>
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption">
                    <span class="caption-subject uppercase">Demographics</span>
                </div>
            </div>
            <div class="portlet-body">
                <form action="#">
                    <div class="form-body">
                        <div class="row">
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">Last Name</label>
                                    <input id="patient_last_name" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">First Name</label>
                                    <input id="patient_first_name" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">Middle Name</label>
                                    <input id="patient_middle_name" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">Phone #</label>
                                    <input id="patient_middle_name" class="form-control mask-phone" />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">SSN</label>
                                    <input id="patient_ssn" class="form-control mask-ssn" />
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">Date of Birth</label>
                                    <input id="patient_dob" class="form-control mask-date2" />
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">Gender</label>
                                    <select id="patient_gender" class="form-control">
                                        <option></option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">Race</label>
                                    <select id="patient_race" class="form-control">
                                        <option></option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">Address 1</label>
                                    <input id="patient_address1" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">Address 2</label>
                                    <input id="patient_address2" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">City</label>
                                    <input id="patient_city" class="form-control" />
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">State</label>
                                    <select id="patient_state" class="form-control">
                                        <option></option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-sm-6 col-md-3">
                                <div class="form-group-sm">
                                    <label class="control-label">Zip</label>
                                    <input id="patient_zip" class="form-control mask-number" />
                                </div>
                            </div>
                        </div>
                        <div class="row margin-top-15">
                            <div class="col-lg-12">
                                <button type="submit" class="btn btn-outline green">Save</button>
                                <button type="button" class="btn btn-outline red">Undo</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="col-sm-6">
        <div class="portlet light bordered">
            <div class="portlet-title">
                <div class="caption">
                    <span class="caption-subject uppercase">Insurance</span>
                </div>
            </div>
            <div class="portlet-body">
                <form action="#">
                    <div class="form-body">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group-sm">
                                    <label class="control-label">Primary</label>
                                    <input id="patient_insurance_primary" class="form-control" placeholder="Type ahead" />
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group-sm">
                                    <label class="control-label">Secondary</label>
                                    <input id="patient_insurance_secondary" class="form-control" placeholder="Type ahead" />
                                </div>
                            </div>                            
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group-sm">
                                    <label class="control-label">Tertiary</label>
                                    <input id="patient_insurance_tertiary" class="form-control" placeholder="Type ahead" />
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group-sm">
                                    <label class="control-label">Guarantor</label>
                                    <input id="patient_insurance_guarantor" class="form-control" placeholder="Type ahead" />
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group-sm">
                                    <label class="control-label">Date of Injury</label>
                                    <input id="patient_insurance_doi" class="form-control mask-date2" />
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group-sm">
                                    <label class="control-label">Pre Auth #</label>
                                    <input id="patient_insurance_pre_auth" class="form-control" />
                                </div>
                            </div>
                        </div>
                        <div class="row patient-insurance-checkboxes">
                            <div class="col-lg-12">
                                <div class="form-group-sm">
                                    <input id="patient_insurance_auto_accident" type="checkbox" class="icheck" />
                                    <label>Auto Accident</label>
                                    
                                </div>
                            </div>
                            <div class="col-lg-12">
                                <div class="form-group-sm">
                                    <input id="patient_insurance_workers_comp" type="checkbox" class="icheck" />
                                    <label class="control-label">Worker's Comp</label>
                                </div>
                            </div>
                        </div>
                        <div class="row margin-top-15">
                            <div class="col-lg-12">
                                <button type="submit" class="btn btn-outline green">Save</button>
                                <button type="button" class="btn btn-outline red">Undo</button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
